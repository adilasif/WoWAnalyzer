    "jsdom": "^22.1.0",
    "lint-staged": "15.2.2",
    "oxc-parser": "0.94.0",
    "prettier": "3.6.2",
    "react-test-renderer": "19.2.0",
    "sass": "1.93.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.12",
    "vite-plugin-svgr": "4.5.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "wow-dbc": "github:RPGLogs/wow-dbc#main"
  },
  "resolutions": {},
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:interface": "pnpm run test --exclude \"src/parser/**/*\"",
    "test:parser": "pnpm run test ./src/parser",
    "typecheck": "tsc",
    "lint-uncached": "eslint --format stylish --max-warnings 0 --no-warn-ignored",
    "lint": "pnpm run lint-uncached --cache .",
    "lint:fix": "pnpm run lint --fix",
    "lint:suppress": "pnpm run lint:fix --suppress-all",
    "lint:prune": "pnpm run lint:fix --prune-suppressions",
    "extract": "lingui extract --overwrite --clean",
    "prepare": "is-ci || husky",
    "e2e:setup": "tsx scripts/e2e/generate-configs.ts",
    "e2e": "pnpm run e2e:setup && playwright test",
    "e2e:codegen": "playwright codegen http://localhost:3000",
    "e2e:report": "playwright show-report",
    "generate-talents": "npx tsx ./scripts/talents/generate-talents.ts && prettier -w src/common/TALENTS/*.ts",
    "generate-talents:ptr": "npx tsx ./scripts/talents/generate-talents.ts --ptr && prettier -w src/common/TALENTS/*.ts",
    "spell-lists:create": "node --experimental-strip-types scripts/spell-lists/generate-list.ts",
    "spell-lists:update": "node --experimental-strip-types scripts/spell-lists/update-all-lists.ts src/analysis/**/*.ts",
    "preview": "vite build && vite preview"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs,json,md,mdx,yml,graphql,scss,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "pnpm run lint:fix"
    ]
  },
  "packageManager": "pnpm@9.5.0",
  "pnpm": {
    "patchedDependencies": {
      "vega-embed@6.20.2": "patches/vega-embed@6.20.2.patch"
    }
  }
}
