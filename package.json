    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.11",
    "@types/react-test-renderer": "^16.9.5",
    "@types/react-textfit": "^1.1.4",
    "@types/react-toggle": "^4.0.5",
    "@types/react-transition-group": "^4.4.10",
    "@types/react-virtualized": "^9.21.30",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react-swc": "4.1.0",
    "@vitest/eslint-plugin": "1.1.38",
    "@vitest/ui": "3.2.4",
    "cross-env": "^7.0.3",
    "decompress": "^4.2.1",
    "dotenv-flow": "^4.1.0",
    "eslint": "9.22.0",
    "eslint-plugin-depend": "0.12.0",
    "eslint-plugin-progress": "0.0.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-wowanalyzer": "workspace:*",
    "glob": "11.0.0",
    "globals": "16.0.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "jiti": "2.4.2",
    "jsdom": "^22.1.0",
    "lint-staged": "15.2.2",
    "oxc-parser": "0.94.0",
    "prettier": "~3.2.5",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.74.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "4.19.3",
    "typescript": "5.4.5",
    "typescript-eslint": "8.26.1",
    "vite": "7.1.10",
    "vite-plugin-svgr": "4.5.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "wow-dbc": "github:RPGLogs/wow-dbc#main"
  },
  "resolutions": {},
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:interface": "pnpm run test --exclude \"src/parser/**/*\"",
    "test:parser": "pnpm run test ./src/parser",
    "typecheck": "tsc",
    "lint-uncached": "eslint --format stylish --max-warnings 0 --no-warn-ignored",
    "lint": "pnpm run lint-uncached --cache .",
    "lint:fix": "pnpm run lint --fix",
    "lint-baseline": "node --experimental-strip-types scripts/lint-baseline/baseline.ts",
    "extract": "lingui extract --overwrite --clean",
    "prepare": "is-ci || husky",
    "e2e:setup": "tsx scripts/e2e/generate-configs.ts",
    "e2e": "pnpm run e2e:setup && playwright test",
    "e2e:codegen": "playwright codegen http://localhost:3000",
    "e2e:report": "playwright show-report",
    "generate-talents": "npx tsx ./scripts/talents/generate-talents.ts && prettier -w src/common/TALENTS/*.ts",
    "generate-talents:ptr": "npx tsx ./scripts/talents/generate-talents.ts --ptr && prettier -w src/common/TALENTS/*.ts",
    "spell-lists:create": "node --experimental-strip-types scripts/spell-lists/generate-list.ts",
    "spell-lists:update": "node --experimental-strip-types scripts/spell-lists/update-all-lists.ts src/analysis/**/*.ts",
    "preview": "vite build && vite preview"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs,json,md,mdx,yml,graphql,scss,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "pnpm run lint-baseline fix"
    ]
  },
  "packageManager": "pnpm@9.5.0",
  "pnpm": {
    "patchedDependencies": {
      "vega-embed@6.20.2": "patches/vega-embed@6.20.2.patch",
      "@lingui/react@5.5.1": "patches/@lingui__react@5.5.1.patch"
    }
  }
}
