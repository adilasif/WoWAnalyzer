    "@types/react-dom": "19.2.2",
    "@types/react-test-renderer": "19.1.0",
    "@types/react-textfit": "^1.1.4",
    "@types/react-toggle": "^4.0.5",
    "@types/react-transition-group": "^4.4.10",
    "@types/react-virtualized": "9.22.2",
