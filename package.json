    "@lingui/conf": "5.5.1",
    "@lingui/format-json": "5.5.1",
    "@lingui/swc-plugin": "5.6.1",
    "@lingui/vite-plugin": "5.5.1",
    "@oxc-project/types": "0.94.0",
    "@playwright/test": "1.54",
    "@sentry/cli": "2.57.0",
