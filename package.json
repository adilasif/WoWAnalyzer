    "@vitest/eslint-plugin": "1.3.24",
    "@vitest/ui": "3.2.4",
    "eslint": "9.38.0",
    "eslint-plugin-depend": "1.3.1",
    "eslint-plugin-progress": "0.0.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-wowanalyzer": "workspace:*",
    "glob": "11.0.0",
    "globals": "16.0.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
